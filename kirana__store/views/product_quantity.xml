<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="product_quality_view_form" model="ir.ui.view">
        <field name="name">product_quality.view.form</field>
        <field name="model">product.quality</field>
        <field name="arch" type="xml">
            <form string="">
                <sheet>
                    <group>
                        <field name="product_id" />
                        <field name="price" />
                        <field name="qty" required="true"/>
                        <field name="saledata" />
                    </group>
                    <notebook>
                        <page name="demo_fields_liness" string="demos fields" editable="botttom">
                            <group>
                                <tree editable='bottom'>
                                    <field name="product_id" />
                                    <field name="price" />
                                    <field name="qty" />
                                    <field name="saledata" />
                                    <field name="type"/>
                                    <field name="totle"/>
                                </tree>
                                <form string='demo fieldss'>
                                    <field name="product_id" />
                                    <field name="price" />
                                    <field name="qty" />
                                    <field name="saledata" />
                                </form>
                            </group>
                        </page>
                    </notebook>
                </sheet>
            </form>
        </field>
    </record>
    <record id="product_quality_view_tree" model="ir.ui.view">
        <field name="name">product_quality.view.tree</field>
        <field name="model">product.quality</field>
        <field name="arch" type="xml">
            <tree string="">
                <field name="product_id" />
                <field name="price" />
                <field name="qty" />
                <field name="saledata" />
                <field name="type"/>
                <field name="totle"/>
                <field name="create_date" />
            </tree>
        </field>
    </record>
    <record id="product_quality_action" model="ir.actions.act_window">
        <field name="name">Product Buy/Sale List</field>
        <field name="res_model">product.quality</field>
        <field name="view_mode">tree,form</field>
        <field name="context">{'dynamic_domains': True}</field>
    </record>
    <menuitem
        id="product_quality_submenus"
        name="Product Qualitys"
        sequence="10"
        action="product_quality_action"
        parent="products_data_menu" />
</odoo>